# Find the sum of all the multiples of 3 or 5 below 1000.


def find_sum(n=3, j=5):
    # set counter for sum
    sum = 0

    for i in range(1000):
        # check if number in range is divisible by input number
        if (i % n == 0) or (i % j == 0):
            sum += i

    return sum


print(find_sum(3))
